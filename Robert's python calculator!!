num1 = int(input("What is your second first number:  "))
op = input("What is your operation:  ")
num2 = int(input("What is your second number: "))

if op == "+":
    print("num1+num2")
if op == "-":
    print(num1-num2)
if op == "*":
    print(num1*num2)
if op == "/":
    print(num1/num2)